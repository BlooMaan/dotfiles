! Option decriptions: http://pod.tst.eu/http://cvs.schmorp.de/rxvt-unicode/doc/rxvt.1.pod

URxvt*font: xft:DejaVu\ Sans\ Mono\ for\ Powerline:pixelsize=14
URxvt*scrollBar: false
URxvt*borderLess: true
URxvt*scrollstyle: plain
URxvt*geometry: 180x50
URxvt*perl-ext-common:  default,tabbed,selection-to-clipboard

URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.tabbed.tabbar-fg: 8
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    2
URxvt.tabbed.tab-bg:    0

! Set transparency
URxvt*depth: 32
#define background_opacity 80

